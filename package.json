{
  "name": "@jambit/wdio-cucumber-selected-steps",
  "version": "0.8.0",
  "description": "Cucumber step definition basics for easy import",
  "homepage": "https://github.com/jambit/wdio-cucumber-selected-steps",
  "bugs": {
    "url": "https://github.com/jambit/wdio-cucumber-selected-steps/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jambit/wdio-cucumber-selected-steps.git"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "test",
    "selenium",
    "selectors",
    "steps"
  ],
  "author": "Santo Pfingsten (Jambit GmbH)",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "eslint": "eslint src --ext ts",
    "eslint:fix": "eslint src --fix --ext ts",
    "docs:html": "documentation build src/support/** --parse-extension ts -f html -o docs",
    "docs:json": "documentation build src/support/** --parse-extension ts -f json -o docs/raw.json",
    "docs:steps": "node stepdocs.js",
    "docs": "npm run docs:html && npm run docs:json && npm run docs:steps"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "chai": "^4.2.0",
    "glob": "^7.1.6",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/cucumber": "^4.0.7",
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.12.7",
    "@types/webdriverio": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "@wdio/cucumber-framework": "^5.16.6",
    "@wdio/sync": "^5.16.5",
    "babel-eslint": "^10.0.3",
    "cucumber": "^5.1.0",
    "documentation": "^12.1.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "typescript": "^3.7.2",
    "webdriverio": "^5.16.6"
  },
  "peerDependencies": {
    "cucumber": "^5.1.0",
    "webdriverio": "^5.16.6",
    "@wdio/cucumber-framework": "^5.16.6",
    "@wdio/sync": "^5.16.5"
  }
}
